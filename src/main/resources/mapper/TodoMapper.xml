<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.scheduler.dao.TodoMapper">

    <select id="getAllTodoList" resultType="TodoDto">
        SELECT * 
        FROM todolist
        WHERE id=1
        ORDER BY id
    </select>


</mapper>
